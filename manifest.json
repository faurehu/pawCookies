{
  "manifest_version": 2,

  "name": "Paw Cookie Collector",
  "description": "A Chrome extension to export your cookies to Paw.",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "devtools_page": "index.html",
  "permissions": [
    "cookies",
    "http://*/*",
    "https://*/*"
  ]
}
